    "paths": {
      "/git": {
        "post": {
          "description": "Create a new Git",
          "operationId": "createGit",
          "parameters": [],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Git"
                }
              }
            },
            "required": true
          },
          "responses": {
            "201": {
              "description": "The user has been created",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Git"
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "orderBy": {
          "type": "string",
          "example": "desc",
          "enum": ["asc", "desc"],
          "default": "desc"
        },
        "orderField": {
          "type": "string",
          "example": "rating",
          "default": "rating"
        },
        "userName": {
          "type": "string",
          "example": "Example Name User"
        },
        "repository": {
          "type": "string",
          "example": "Example name repository user"
        },
        "Git": {
          "type": "object",
          "properties": {
            "userName": {
              "$ref": "#/components/schemas/userName"
            },
            "repository": {
              "$ref": "#/components/schemas/repository"
            }
          }
        }
      }
